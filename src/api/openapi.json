        "required": [
          "name"
        ],
        "title": "ActivateWorkspaceRequest"
      },
      "ActiveWorkspace": {
        "properties": {
          "name": {
            "type": "string",
            "title": "Name"
          },
          "is_active": {
            "type": "boolean",
            "title": "Is Active"
          },
          "last_updated": {
            "title": "Last Updated"
          }
        },
        "type": "object",
        "required": [
          "name",
          "is_active",
          "last_updated"
        ],
        "title": "ActiveWorkspace"
