              "$ref": "#/components/schemas/Workspace"
            },
            "type": "array",
            "title": "Workspaces"
          }
        },
        "type": "object",
        "required": [
          "workspaces"
        ],
        "title": "ListWorkspacesResponse"
      },
      "QuestionAnswer": {
        "properties": {
          "question": {
            "$ref": "#/components/schemas/ChatMessage"
          },
          "answer": {
            "anyOf": [
              {
                "$ref": "#/components/schemas/ChatMessage"
              },
              {
                "type": "null"
              }
            ]
          }
        },
        "type": "object",
        "required": [
          "question",
          "answer"
        ],
